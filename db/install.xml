<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/dash/db" VERSION="20191109" COMMENT="XMLDB file for Moodle blocks/dash"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="dash_template" COMMENT="Template definitions">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="Template name"/>
        <FIELD NAME="available_field_definitions" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="Field definitions that this template will display"/>
        <FIELD NAME="query_template" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="layout_type" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" COMMENT="Layout type"/>
        <FIELD NAME="layout_path" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Moodle template path component/template_name"/>
        <FIELD NAME="layout_mustache" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Raw mustache template code"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>