{{< block_dash/layout }}
    {{$body}}
        {{^data.rows}}
            <p class="text-muted">{{#str}}noresults{{/str}}</p>
        {{/data.rows}}
        <div class="table-responsive">
            <table class="table">
                {{#data.rows}}
                    <tr>
                        {{#data}}
                            <td>{{get_value}}</td>
                        {{/data}}
                    </tr>
                {{/data.rows}}
            </table>
        </div>
    {{/body}}
{{/ block_dash/layout }}
